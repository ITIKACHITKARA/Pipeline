pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               sh 'docker build -t marchdocker .'
            }
        }
        stage('run') {
            steps {
                 sh 'docker run -it marchdocker'    }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
